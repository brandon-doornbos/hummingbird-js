{
    "source": {
        "include": ["src", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "dark",
			"menu": [
				{
					"title": "Trello Board",
					"link": "https://trello.com/b/QO8sjFeJ/hummingbird",
					"target": "_blank"
				},
				{
					"title": "Github",
					"link": "https://www.github.com/brandon-doornbos/hummingbird-js/",
					"target": "_blank"
				},
				{
					"title": "Library",
					"link": "https://projects.brandond.nl/Hummingbird/dist/hummingbird.js",
					"target": "_blank"
				},
				{
					"title": "Minified Library",
					"link": "https://projects.brandond.nl/Hummingbird/dist/hummingbird.min.js",
					"target": "_blank"
				}
			]
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
